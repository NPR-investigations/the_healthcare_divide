    "def clean_tuple(tup):\n",
    "    string = str(tup).strip().replace(\"(\",\"\").replace(\")\",\"\").replace(\",\",\"_\").replace(\"/\",\"_\").replace(\"'\",\"\").replace(\" \",\"\").strip()\n",
    "    return(string)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "accompanied-appreciation",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_16_sheet(path, year):\n",
    "    x2017 = pd.ExcelFile(path)\n",
    "    raw2017A = x2017.parse('dbo_JARHospitalScheduleA')\n",
    "    raw2015Aptnames = ['StateID', 'FacilityID', 'FacilityName', 'MedicareID', 'ZipCode','City']\n",
    "    raw2017Apt = raw2017A[raw2015Aptnames].copy().drop_duplicates()\n",
    "\n",
    "    raw2017B = x2017.parse(\"dbo_JARHospitalScheduleB\")\n",
    "    raw2015Bptnames = ['OrganizationGroup',\n",
    "     'BelongToHealthGroup',\n",
    "     'HealthGroupName',\n",
    "     'FacilityID',\n",
    "     'CategoryText',\n",
    "    'HealthGroupCity']\n",
    "    raw2017Bpt = raw2017B[raw2015Bptnames].copy().drop_duplicates()\n",
    "\n",
    "    raw2017Ea = x2017.parse('dbo_JARHospitalScheduleEa')\n",
    "    raw2017Eanames = ['ChargeText','ChargesSubType','GrossCharge','AdjCharge','NetCharge','FacilityID']\n",
    "    raw2017Eapt = raw2017Ea[raw2017Eanames].copy().drop_duplicates()\n",
    "    chargelist = ['Medicaid/TennCare Inpatient',\n",
    "                 'Medicaid/TennCare Outpatient',\n",
    "                 'Grand Total',\n",
    "                 'Total Bad Debt',\n",
    "                 'Total Charity',\n",
    "                 'Total Charity plus Bad Debt',\n",
    "                 'TOTAL REVENUE',\n",
    "                 'Total Inpatient (excludes Normal  Newborn)',\n",
    "                'Total Inpatient (includes Normal Newborn)',\n",
    "                 'Total Outpatient']\n",
    "\n",
    "    raw2017Eapt1 = raw2017Eapt[(raw2017Eapt.ChargeText.isin(chargelist)) & (pd.isna(raw2017Eapt.ChargesSubType) == True) ]\n",
    "    raw2017Eapt1pv = raw2017Eapt1.pivot(index = 'FacilityID', columns = 'ChargeText')\n",
    "    raw2017Eapt1pv.columns = raw2017Eapt1pv.columns.to_flat_index()\n",
    "    raw2017Eapt1pv = raw2017Eapt1pv.reset_index()\n",
    "    raw2017Eapt1pv1 = raw2017Eapt1pv.dropna(how = 'all', axis = 1).copy().drop_duplicates()\n",
    "    raw2017Eapt1pv1.columns = [str(x) for x in raw2017Eapt1pv1.columns]\n",
    "    raw2017Eapt1pv1.columns = raw2017Eapt1pv1.columns.str.replace(\"'\",\"\").str.replace(\"(\",\"\").str.replace(\")\",\"\").str.replace(\",\",\"_\").str.replace(\" \",\"\").str.replace('/','_').str.strip()\n",
    "    raw2017Eapt1pv1['medicaid_charges'] = raw2017Eapt1pv1.GrossCharge_Medicaid_TennCareInpatient + raw2017Eapt1pv1.GrossCharge_Medicaid_TennCareOutpatient\n",
    "    raw2017Eapt1pv1['medicaid_revenue'] = raw2017Eapt1pv1.NetCharge_Medicaid_TennCareInpatient + raw2017Eapt1pv1.NetCharge_Medicaid_TennCareOutpatient\n",
    "\n",
    "    raw2017Eb = x2017.parse('dbo_JARHospitalScheduleEb')\n",
    "    raw2017Ebpt = raw2017Eb[['ExpenseText','GrossExpenses','FacilityID']].copy().drop_duplicates()\n",
    "    raw2017Ebpt1 = raw2017Ebpt[raw2017Ebpt.ExpenseText == 'TOTAL EXPENSES'].copy().drop_duplicates()\n",
    "    raw2017Ebpt2 = raw2017Ebpt1.drop('ExpenseText', axis = 1)\n",
    "\n",
    "    raw2017F = x2017.parse(\"dbo_JARHospitalScheduleF\")\n",
    "    raw2017Fpt = raw2017F[['NumLicensedBeds','FacilityID']].copy().drop_duplicates()\n",
    "    raw2017G = x2017.parse('dbo_JARHospitalScheduleG3')\n",
    "    raw2017Gpt = raw2017G[['PatientDays','NumVisits','PayorText','FacilityID']].copy().drop_duplicates()\n",
    "    payorlist = ['Medicaid/TennCare InPatient','Medicaid/TennCare OutPatient','Total Payor']\n",
    "    raw2017Gpt1 = raw2017Gpt[raw2017Gpt.PayorText.isin(payorlist)].copy().drop_duplicates()\n",
    "    raw2017Gpt1pv = raw2017Gpt1.pivot(index = 'FacilityID', columns = 'PayorText')\n",
    "    raw2017Gpt1pv1 = raw2017Gpt1pv.dropna(how = 'all', axis = 1)\n",
    "    raw2017Gpt1pv1.columns = raw2017Gpt1pv1.columns.to_flat_index()\n",
    "    raw2017Gpt1pv1.columns = [clean_tuple(x) for x in raw2017Gpt1pv1.columns ]\n",
    "    raw2017Gpt1pv1['medicaid_pct_inpatient'] = raw2017Gpt1pv1.PatientDays_Medicaid_TennCareInPatient/raw2017Gpt1pv1.PatientDays_TotalPayor\n",
    "    raw2017Gpt1pv1['medicaid_pct_outpatient'] = raw2017Gpt1pv1.NumVisits_Medicaid_TennCareOutPatient/raw2017Gpt1pv1.NumVisits_TotalPayor\n",
    "\n",
    "    raw2017P = x2017.parse(\"dbo_JARHospitalScheduleP\")\n",
    "    raw2017Ppt = raw2017P[['TotUnInsPat','TotNumPatNoIns','CostToChargeRatio','FacilityID']].copy().drop_duplicates()\n",
    "\n",
    "    dflist2017 = [raw2017Apt,raw2017Bpt,raw2017Eapt1pv1,raw2017Ebpt2,raw2017Fpt,raw2017Gpt1pv1,raw2017Ppt]\n",
    "    \n",
    "    jardoh2017 = reduce(lambda left,right: pd.merge(left, right,on = 'FacilityID', how = 'outer'), dflist2017)\n",
    "    jardoh2017['year'] = year\n",
    "    return(jardoh2017)"
   ]
