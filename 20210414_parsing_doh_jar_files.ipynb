   "outputs": [],
   "source": [
    "def get_15_sheet(path,year):\n",
    "    x2015 = pd.ExcelFile(path)\n",
    "    raw2015A = x2015.parse(\"ScheduleA\")\n",
    "    try:\n",
    "        raw2015Apt = raw2015A[['HospitalID','HospitalName','HospitalMedicareID','HospitalZipCode','HospitalCity']].copy().drop_duplicates()\n",
    "    except Exception:\n",
    "        raw2015Apt = raw2015A[['HospitalID','HospitalName','HospitalZipCode','HospitalCity']].copy().drop_duplicates()\n",
    "\n",
    "    \n",
    "    raw2015B = x2015.parse(\"ScheduleB\")\n",
    "    raw2015Bpt = raw2015B[['HospitalID','ControlOrganizationCode','ControlOrganization','HealthSystem','Headquarters','ServiceCategoryCode','ServiceCategory','HeadquartersCity']].copy().drop_duplicates()\n",
    "    \n",
    "    raw2015E = x2015.parse(\"ScheduleE\")\n",
    "    raw2015Ept = raw2015E[['HospitalID','ChargesSubCode','ChargesSub','ChargesGrossPatient','ChargesAdjustments','RevenueNetPatient','RevenueOther','Expenses']].copy().drop_duplicates()\n",
    "    # extracting the subcodes for pivot\n",
    "    subcodelist = ['A4e','A1c','A1d','A5f','A6h','A6i','B2i','A8f','A4a','A4d','A4c']\n",
    "    raw2015Ept1 = raw2015Ept[raw2015Ept.ChargesSubCode.isin(subcodelist)].copy().drop_duplicates()\n",
    "    raw2015Ept1pv = raw2015Ept1.pivot(columns = 'ChargesSubCode', values = ['ChargesGrossPatient','ChargesAdjustments','RevenueNetPatient','RevenueOther','Expenses'], index = 'HospitalID')\n",
    "    raw2015Ept1pv.columns = raw2015Ept1pv.columns.to_flat_index()\n",
    "    raw2015Ept1pv = raw2015Ept1pv.reset_index()\n",
    "    raw2015Ept1pv.columns = [str(x) for x in raw2015Ept1pv.columns]\n",
    "    raw2015Ept1pv.columns = raw2015Ept1pv.columns.str.replace(\"'\",\"\").str.replace(\"(\",\"\").str.replace(\")\",\"\").str.replace(\",\",\"_\").str.replace(\" \",\"\").str.strip()\n",
    "    raw2015Ept1pvlist = ['HospitalID', 'ChargesGrossPatient_A1c', 'ChargesGrossPatient_A1d',\n",
    "       'ChargesGrossPatient_A4e', 'ChargesAdjustments_A5f',\n",
    "       'ChargesAdjustments_A6h', 'ChargesAdjustments_A6i',\n",
    "       'RevenueNetPatient_A1c', 'RevenueNetPatient_A1d',\n",
    "       'RevenueNetPatient_A4e', 'RevenueOther_A8f', 'Expenses_B2i',\n",
    "        'ChargesGrossPatient_A4a','ChargesGrossPatient_A4d',\n",
    "                        'ChargesGrossPatient_A4c']\n",
    "    raw2015Ept1pv1 = raw2015Ept1pv[raw2015Ept1pvlist].copy().drop_duplicates()\n",
    "    raw2015Ept1pv1['medicaid_charges'] = raw2015Ept1pv1.ChargesGrossPatient_A1c + raw2015Ept1pv1.ChargesGrossPatient_A1d\n",
    "    raw2015Ept1pv1['medicaid_revenue'] = raw2015Ept1pv1.RevenueNetPatient_A1c + raw2015Ept1pv1.RevenueNetPatient_A1d\n",
    "    raw2015Ept1pv1 = raw2015Ept1pv1.rename(columns = {\"ChargesGrossPatient_A4e\":\"total_charges\",\n",
    "                                                      \"RevenueNetPatient_A4e\":\"net_patient_revenue\",\n",
    "                                                     \"RevenueOther_A8f\":\"total_revenue\",\n",
    "                                                     \"Expenses_B2i\":\"total_expenses\",\n",
    "                                                     \"ChargesAdjustments_A5f\":\"bad_debt\",\n",
    "                                                     \"ChargesAdjustments_A6h\":\"total_charity\",\n",
    "                                                     \"ChargesAdjustments_A6i\":\"bad_debt_charity\",\n",
    "                                                     \"ChargesGrossPatient_A4a\":\"total_inpatient_charge\",\n",
    "                                                     \"ChargesGrossPatient_A4d\":\"total_outpatient_charge\",\n",
    "                                                     \"ChargesGrossPatient_A4c\":\"total_inpatient_charge_newborn\"})\n",
    "\n",
    "    raw2015F = x2015.parse(\"ScheduleF\")\n",
    "    raw2015Fpt = raw2015F[['HospitalID','BedsLicensedTotal']].copy().drop_duplicates()\n",
    "    \n",
    "    raw2015G  = x2015.parse(\"ScheduleG\")\n",
    "    raw2015Gpt = raw2015G.iloc[:,[0,2,3,10,11]].copy().drop_duplicates()\n",
    "    payerlist = [10,5]\n",
    "    raw2015Gpt1 = raw2015Gpt[raw2015Gpt.AdmisDischPayerCode.isin(payerlist)].copy().drop_duplicates()\n",
    "    raw2015Gpt1pv = raw2015Gpt1.pivot(index = 'HospitalID', columns = 'AdmisDischPayerCode',values = ['AdmisDischDays','AdmisDischVisits'])\n",
    "    raw2015Gpt1pv.columns = raw2015Gpt1pv.columns.to_flat_index()\n",
    "    raw2015Gpt1pv = raw2015Gpt1pv.reset_index()\n",
    "    raw2015Gpt1pv.columns = [str(x) for x in raw2015Gpt1pv.columns]\n",
    "    raw2015Gpt1pv.columns = raw2015Gpt1pv.columns.str.replace(\"'\",\"\").str.replace(\"(\",\"\").str.replace(\")\",\"\").str.replace(\",\",\"_\").str.replace(\" \",\"\").str.replace('.','_').str.replace('_0','').str.strip()    \n",
    "    raw2015Gpt1pv['medicaid_pct_inpatient'] = raw2015Gpt1pv.AdmisDischDays_5/raw2015Gpt1pv.AdmisDischDays_10\n",
    "    raw2015Gpt1pv['medicaid_pct_outpatient'] = raw2015Gpt1pv.AdmisDischVisits_5/raw2015Gpt1pv.AdmisDischVisits_10\n",
    "    \n",
    "    dflist2015 = [raw2015Apt,raw2015Bpt,raw2015Ept1pv1,raw2015Fpt,raw2015Gpt1pv]\n",
    "    jardoh2015 = reduce(lambda left,right: pd.merge(left, right,on = 'HospitalID', how = 'outer'), dflist2015)\n",
    "    jardoh2015['year'] = year\n",
    "    jardoh2015 = jardoh2015.rename(columns = {\"HospitalID\":'state_id'})\n",
    "    return(jardoh2015)"
   ]
  },
